 if(!viewMore) {\r\n        ref.limitToLast(1).once('value', snapshot => {\r\n\r\n          snapshot.forEach(item => {\r\n            arrayPosts.push(item.val());\r\n          })\r\n\r\n          setCommentOnPost(arrayPosts);\r\n        })\r\n      }\r\n\r\n      else {\r\n        ref.once('value', snapshot => {\r\n\r\n          snapshot.forEach(item => {\r\n            arrayPosts.push(item.val());\r\n          })\r\n\r\n          setCommentOnPost(arrayPosts);\r\n        })\r\n      }\r\n\r\n    }\r\n\r\n    /**\r\n     * Check the database how many comments one post has.\r\n     */\r\n    const checkForMoreComments = () => {\r\n      ref.once(\"value\", snapshot => {\r\n        let children = snapshot.numChildren();\r\n        setCommentCounter(children);\r\n         });\r\n    }\r\n\r\n    /**\r\n     * UPDATE ME\r\n     */\r\n    const retriveKeys = () => {\r\n      firebase.database().ref('comments/' + databaseKey).once(\"value\", snapshot => {\r\n        let arrayKeys = [];\r\n\r\n        snapshot.forEach(item => {\r\n          arrayKeys.push(item.key);\r\n        })\r\n\r\n        setCommentOnPostId(arrayKeys);\r\n      });\r\n    }\r\n\r\n    commentView();\r\n    checkForMoreComments();\r\n    retriveKeys();\r\n\r\n  }, [firebase, databaseKey, postId, viewMore])\r\n\r\n\r\n  return(\r\n  <div className=\"comment-wall\">\r\n    <ul>\r\n    {Object.values(commentOnPost).map(({content}, i) => (\r\n        <li key={i}>\r\n          <div className=\"comment-avatar\">\r\n            <ProfileAvatar\r\n              firstName={firstName}\r\n              lastName={lastName}\r\n              size='small'\r\n            />\r\n          </div>\r\n\r\n          <div className=\"comment-wall-chatbox\">\r\n            <small className=\"comment-first-last-name\">\r\n              <a href=\"/#\">{firstName} {lastName}</a>\r\n            </small>\r\n\r\n            <div className=\"comment-content\">\r\n              {textFieldOn ?\r\n                <TextField\r\n                  autoFocus\r\n                  value={content}\r\n                  type=\"text\"\r\n                  // onChange={(e) => setTextValue(e.target.value)}\r\n                  autoComplete=\"off\"\r\n                  InputProps={{\r\n                    className: classes.textField\r\n                  }}\r\n                />\r\n                  :\r\n                <small>{content}</small> }\r\n            </div>\r\n          </div>\r\n\r\n            <div className=\"comment-more\">\r\n              <IconButton aria-label=\"settings\">\r\n                <Popover\r\n                  postId={databaseKey}\r\n                  commentId={commentOnPostId[i]}\r\n                  firebase={firebase}\r\n                  firstBox={'Edit'}\r\n                  secondBox={'Delete'}\r\n                  functionality={'comment'}\r\n                />\r\n              </IconButton>\r\n            </div>\r\n            <br />\r\n            {commentCounter > 1 ? <u className=\"comment-view-more-comment\" onClick={showMoreComment}>{message}</u> : '' }\r\n\r\n        </li>\r\n    ))}\r\n    </ul>\r\n  </div>\r\n  );\r\n}\r\n\r\nexport default CommentWall;","/mnt/c/Users/lex_0/Desktop/React Projects/socialmedia/src/components/popover.js",[],{"ruleId":"81","replacedBy":"82"},{"ruleId":"83","replacedBy":"84"},{"ruleId":"81","replacedBy":"85"},{"ruleId":"83","replacedBy":"86"},{"ruleId":"81","replacedBy":"87"},{"ruleId":"83","replacedBy":"88"},{"ruleId":"89","severity":1,"message":"90","line":41,"column":9,"nodeType":"91","messageId":"92","endLine":41,"endColumn":27},"no-native-reassign",["93"],"no-negated-in-lhs",["94"],["93"],["94"],["93"],["94"],"no-unused-vars","'triggerEditComment' is assigned a value but never used.","Identifier","unusedVar","no-global-assign","no-unsafe-negation"]