n\r\n        <Dialog open={open} onClose={handleClose} aria-labelledby=\"form-dialog-title\" fullWidth>\r\n          <DialogActions>\r\n            <Button className=\"close-button\" onClick={() => setOpen(false)} >\r\n              <HighlightOffIcon />\r\n            </Button>\r\n            </DialogActions>\r\n          <DialogTitle id=\"form-dialog-title\">{title}</DialogTitle>\r\n\r\n          <DialogContent>\r\n              <div className=\"content-header\">\r\n                <ProfileAvatar firstName={firstName} lastName={lastName}/>\r\n                  <div className=\"name\">\r\n                    {firstName} {lastName}\r\n                  </div>\r\n                </div>\r\n                <br />\r\n\r\n            <TextField\r\n              autoFocus\r\n              margin=\"dense\"\r\n              id=\"name\"\r\n              placeholder=\"Whats on your mind?\"\r\n              type=\"text\"\r\n              fullWidth\r\n              onChange={(e) => setTextValue(e.target.value)}\r\n              value={textValue}\r\n              autoComplete=\"off\"\r\n            />\r\n          </DialogContent>\r\n\r\n          <DialogActions>\r\n            <Button\r\n              fullWidth\r\n              color=\"primary\"\r\n              variant=\"contained\"\r\n              onClick={post}\r\n              disabled={textValue.length === 0}\r\n            >\r\n              Post\r\n            </Button>\r\n          </DialogActions>\r\n          <p className=\"alert\">{error}</p>\r\n        </Dialog>\r\n\r\n    <Posts\r\n      firstName={firstName}\r\n      lastName={lastName}\r\n      databasePosts={databasePosts}\r\n      editPost={editPost}\r\n    />\r\n\r\n    </div>\r\n  )\r\n}\r\n\r\nexport default Create;","/mnt/c/Users/lex_0/Desktop/React Projects/socialmedia/src/components/posts.js",[],"/mnt/c/Users/lex_0/Desktop/React Projects/socialmedia/src/components/profileAvatar.js",[],["52","53"],"/mnt/c/Users/lex_0/Desktop/React Projects/socialmedia/src/